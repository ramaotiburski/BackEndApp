<table class="table">
    <thead>
        <tr>
            <th scope="col">Marca</th>
            <th scope="col">Ano</th>
            <th scope="col">Usado</th>
        </tr>
    </thead>
    <tbody>
        {{#each carro}}
        <tr>
            <th>{{marca}}</th>
            <td>{{ano}}</td>
            <td>{{usado}}</td>
            <td>
                <a href="/editar/{{idcarro}}"><button type="submit" class="btn btn-warning">Editar</button></a>
                <a href="/remover/{{idcarro}}"><button type="submit" class="btn btn-danger">Apagar</button></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
